{
  "manifest_version": 2,
  "short_name": "Astartup",
  "name": "Astartup",
  "description": "Mission Control Center browser extension for the A* Startup Network and Astartup Live-stream.",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "events.js"
    ]
  },
  "browser_action":{
    "default_title": "Astartup Mission Control Center",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}
